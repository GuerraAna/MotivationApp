<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.motivationapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/motivap_ic_launcher"
        android:label="@string/motivap_app_name"
        android:roundIcon="@mipmap/motivap_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/motivap_Theme.MotivationApp">
        <activity
            android:name=".ErrorActivity"
            android:exported="false" />
        <activity
            android:name=".UserActivity"
            android:exported="false" />
        <activity
            android:name=".activity.HomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/motivap_preloaded_fonts" />
    </application>

</manifest>