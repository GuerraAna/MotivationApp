<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="motivap_Theme.MotivationApp" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/motivap_dark_gray</item>
        <item name="colorPrimaryVariant">@color/motivap_another_dark_gray</item>
        <item name="colorOnPrimary">@color/motivap_white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/motivap_teal_200</item>
        <item name="colorSecondaryVariant">@color/motivap_teal_700</item>
        <item name="colorOnSecondary">@color/motivap_black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>